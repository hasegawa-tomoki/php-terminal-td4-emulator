{
  "name": "hasegawa-tomoki/php-terminal-td4-emulator",
  "description": "A PHP terminal TD4 emulator",
  "keywords": ["php", "terminal", "console", "td4", "emulator"],
  "homepage": "http://github.com/hasegawa-tomoki/php-terminal-td4-emulator",
  "license": "MIT",
  "authors": [
    {
      "name": "HASEGAWA Tomoki",
      "email": "hasegawa.tomoki@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.0",
    "pear/console_commandline": "^1.2"
  },
  "require-dev": {
    "phpunit/phpunit": "*"
  },
  "autoload": {
    "psr-4": {
      "Td4\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Td4\\Test\\": "tests"
    }
  },
  "scripts": {
    "cs-check": "phpcs --colors -p --standard=PSR2 ./src ./tests",
    "cs-fix": "phpcbf --colors --standard=PSR2 ./src ./tests",
    "test": "./vendor/bin/phpunit --colors=always"
  },
  "prefer-stable": true
}
